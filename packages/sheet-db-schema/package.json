{
  "name": "sheet-db-schema",
  "version": "0.0.0",
  "private": true,
  "keywords": [],
  "author": "",
  "files": [
    "./dist/**"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "./zero": {
      "types": "./dist/zero.d.mts",
      "default": "./dist/zero.mjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "format": "oxfmt --check",
    "format:apply": "oxfmt",
    "lint": "oxlint .",
    "typecheck": "tsgo --noEmit",
    "build": "drizzle-zero generate --output ./src/zero/schema.ts --skip-types --skip-declare --suppress-defaults-warning && tsdown -c tsdown.config.ts"
  },
  "dependencies": {
    "@rocicorp/zero": "^0.25.9",
    "drizzle-orm": "^0.45.1",
    "drizzle-zero": "^0.17.2",
    "effect": "^3.19.8",
    "postgres": "^3.4.7",
    "typhoon-core": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^24.7.2",
    "@typescript/native-preview": "latest",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.4",
    "oxfmt": "^0.26.0",
    "oxlint": "^1.41.0",
    "pathe": "^2.0.3",
    "tsdown": "^0.16.4"
  }
}
