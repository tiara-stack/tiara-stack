{
  "name": "vibecord",
  "version": "0.0.0",
  "private": true,
  "keywords": [],
  "author": "",
  "files": [
    "./dist/**"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "./register": {
      "types": "./dist/register.d.mts",
      "default": "./dist/register.mjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "format": "oxfmt --check",
    "format:apply": "oxfmt",
    "lint": "oxlint .",
    "typecheck": "tsgo --noEmit",
    "build": "tsdown -c tsdown.config.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@effect/sql-drizzle": "^0.48.0",
    "@opencode-ai/sdk": "^1.1.64",
    "better-sqlite3": "^11.6.0",
    "c12": "^3.3.3",
    "diff": "^8.0.3",
    "discord.js": "^14.18.0",
    "drizzle-orm": "^0.45.1",
    "effect": "^3.19.8",
    "simple-git": "^3.30.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.12",
    "@types/node": "^24.7.2",
    "@typescript/native-preview": "latest",
    "drizzle-kit": "^0.30.0",
    "oxfmt": "^0.26.0",
    "oxlint": "^1.41.0",
    "pathe": "^2.0.3",
    "tsdown": "^0.16.4"
  }
}
