{
  "name": "typhoon-core",
  "version": "0.0.0",
  "private": true,
  "keywords": [],
  "author": "",
  "files": [
    "./dist/**"
  ],
  "type": "module",
  "sideEffects": [],
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "./*": {
      "types": "./dist/*/index.d.mts",
      "default": "./dist/*/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "format": "oxfmt --check",
    "format:apply": "oxfmt",
    "lint": "oxlint .",
    "typecheck": "tsgo --noEmit",
    "build": "tsdown -c tsdown.config.ts"
  },
  "dependencies": {
    "@rocicorp/zero": "^0.25.9",
    "@standard-schema/spec": "^1.0.0"
  },
  "devDependencies": {
    "@typescript/native-preview": "latest",
    "glob": "^11.0.3",
    "oxfmt": "^0.26.0",
    "oxlint": "^1.41.0",
    "pathe": "^2.0.3",
    "tsdown": "^0.16.4"
  },
  "peerDependencies": {
    "effect": "^3.19.8"
  }
}
