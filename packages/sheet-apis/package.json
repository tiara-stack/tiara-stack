{
  "name": "sheet-apis",
  "version": "0.0.0",
  "private": true,
  "keywords": [],
  "author": "",
  "files": [
    "./dist/**"
  ],
  "type": "module",
  "sideEffects": [],
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "./api": {
      "types": "./dist/api.d.mts",
      "default": "./dist/api.mjs"
    },
    "./schema": {
      "types": "./dist/schema.d.mts",
      "default": "./dist/schema.mjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "format": "oxfmt --check",
    "format:apply": "oxfmt",
    "lint": "oxlint .",
    "typecheck": "tsgo --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "build": "tsdown -c tsdown.config.ts && tar -acf dist.tar.zst ./dist"
  },
  "dependencies": {
    "@effect/cluster": "^0.56.1",
    "@effect/experimental": "^0.58.0",
    "@effect/opentelemetry": "^0.61.0",
    "@effect/platform": "^0.94.2",
    "@effect/platform-node": "^0.104.1",
    "@effect/rpc": "^0.73.0",
    "@effect/sql": "^0.49.0",
    "@effect/vitest": "^0.27.0",
    "@effect/workflow": "^0.16.0",
    "@googleapis/sheets": "^10.0.1",
    "@opentelemetry/exporter-prometheus": "^0.207.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.207.0",
    "@opentelemetry/sdk-metrics": "^2.2.0",
    "@opentelemetry/sdk-trace-base": "^2.2.0",
    "@rocicorp/zero": "^0.25.9",
    "arkregex": "^0.0.2",
    "dfx": "^0.127.0",
    "dfx-discord-utils": "workspace:*",
    "effect": "^3.19.8",
    "google-auth-library": "^10.5.0",
    "jose": "^6.1.3",
    "playwright": "^1.58.2",
    "scule": "^1.3.0",
    "sheet-auth": "workspace:*",
    "sheet-db-schema": "workspace:*",
    "typhoon-core": "workspace:*",
    "ufo": "^1.6.1",
    "unstorage": "^1.15.0",
    "vitest": "^4.0.18"
  },
  "devDependencies": {
    "@types/node": "^24.7.2",
    "@typescript/native-preview": "latest",
    "dotenv": "^17.2.3",
    "glob": "^11.0.1",
    "oxfmt": "^0.26.0",
    "oxlint": "1.41.0",
    "pathe": "^2.0.3",
    "tsdown": "^0.16.4"
  }
}
